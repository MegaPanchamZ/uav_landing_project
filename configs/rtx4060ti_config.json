{
    "model": {
        "name": "EdgeLandingNet",
        "num_classes": 6,
        "input_size": 512,
        "uncertainty_estimation": false
    },
    "training": {
        "epochs": 30,
        "batch_size": 8,
        "learning_rate": 0.001,
        "weight_decay": 1e-4,
        "gradient_accumulation_steps": 1,
        "mixed_precision": true,
        "num_workers": 4,
        "pin_memory": true
    },
    "dataset": {
        "patch_size": 512,
        "stride_factor": 0.5,
        "min_valid_pixels": 0.1,
        "augmentation": true,
        "cache_patches": true
    },
    "optimization": {
        "gpu_memory_fraction": 0.9,
        "deterministic": false,
        "benchmark": true
    },
    "scheduler": {
        "type": "cosine",
        "warmup_epochs": 2,
        "min_lr": 1e-6
    }
} 